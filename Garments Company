import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int i;
        int a[]=new int[n];
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        int count=0;
        for(i=0;i<n;i++){
        int k=(int)Math.sqrt(a[i]);
        if(k*k==a[i]){
            count++;
        }
        }
    
            System.out.println(count);
}
}
