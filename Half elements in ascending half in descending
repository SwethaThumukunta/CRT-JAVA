import java.util.*;
class Descending{
public static void main(String args[]){
Scanner s=new Scanner(System.in);
int i;
int n=s.nextInt();
int a[]=new int[n];
for(i=0;i<n;i++){
a[i]=s.nextInt();
}
Arrays.sort(a);
for(i=n-1;i>=n/2;i--){
System.out.print(a[i]+" ");
}
for(i=0;i<n/2;i++){
System.out.print(a[i]+" ");
}
}
}
