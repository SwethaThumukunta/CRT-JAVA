import java.util.*;
class SqSortedArray{
public static void main(String args[]){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int i,j;
int a[]=new int[n];
for(i=0;i<n;i++){
a[i]=s.nextInt();
}
for(i=0;i<n;i++){
a[i]=a[i]*a[i];
}
Arrays.sort(a);
for(i=0;i<n;i++){
System.out.print(a[i]+" ");
}
}
}
